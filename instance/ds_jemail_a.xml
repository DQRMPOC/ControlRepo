<instance>
	<instance>ds_jemail_a</instance>
	<process>DS_LAUNCH</process>
	<description>Launches the Java Email Service</description>
	<inherit>0</inherit>
	<param>
		<parameter>blockOnCommand</parameter>
		<paramvalue>false</paramvalue>
	</param>
	<param>
		<parameter>command</parameter>
		<paramvalue>./run</paramvalue>
	</param>
	<param>
		<parameter>dc_host</parameter>
		<paramvalue>ENV=ds_jemail_a_HOST=</paramvalue>
	</param>
	<param>
		<parameter>dc_port</parameter>
		<paramvalue>ENV=ds_jemail_a_PORT=</paramvalue>
	</param>
	<param>
		<parameter>dc_reusePort</parameter>
		<paramvalue>false</paramvalue>
	</param>
	<param>
		<parameter>dc_taskset</parameter>
		<paramvalue>ENV=ds_jemail_a_TASKSET=</paramvalue>
	</param>
	<param>
		<parameter>parameterConfig</parameter>
		<paramvalue>DS_LAUNCH_COMMANDLINE_PARAMS:ds_jemail</paramvalue>
	</param>
	<param>
		<parameter>parameterText</parameter>
		<paramvalue>--java.memory.xms=32 --java.memory.xmx=64</paramvalue>
	</param>
	<param>
		<parameter>runInBackground</parameter>
		<paramvalue>true</paramvalue>
	</param>
	<param>
		<parameter>workingDirectory</parameter>
		<paramvalue>ENV=JEMAIL_HOME=/</paramvalue>
	</param>
	<instancevariable>
		<variable>*</variable>
		<typ>object</typ>
	</instancevariable>
	<shutdown>
		<shutdownType>unexpected</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
	<shutdown>
		<shutdownType>proper</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
	<shutdown>
		<shutdownType>dc_timeoutAction</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
</instance>
