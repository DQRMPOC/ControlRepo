<instance>
	<instance>emea_tr_ms_b</instance>
	<process>DS_DMSERVER</process>
	<description>Delta Messaging server - allows DM clients to find each other, matches publishers with subscribers.</description>
	<inherit>0</inherit>
	<param>
		<parameter>broadcastPublish</parameter>
		<paramvalue>true</paramvalue>
	</param>
	<param>
		<parameter>dc_gc</parameter>
		<paramvalue>Immediate</paramvalue>
	</param>
	<param>
		<parameter>dc_host</parameter>
		<paramvalue>ENV=emea_tr_ms_b_HOST=</paramvalue>
	</param>
	<param>
		<parameter>dc_port</parameter>
		<paramvalue>ENV=emea_tr_ms_b_PORT=</paramvalue>
	</param>
	<param>
		<parameter>dc_taskset</parameter>
		<paramvalue>ENV=emea_tr_ms_b_TASKSET=</paramvalue>
	</param>
	<param>
		<parameter>initialStateFunct</parameter>
		<paramvalue>.daas.process.init</paramvalue>
	</param>
	<param>
		<parameter>useUnixDomainSockets</parameter>
		<paramvalue>false</paramvalue>
	</param>
	<instancevariable>
		<variable>*</variable>
		<typ>object</typ>
	</instancevariable>
	<shutdown>
		<shutdownType>unexpected</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
	<shutdown>
		<shutdownType>proper</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
	<shutdown>
		<shutdownType>dc_timeoutAction</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
</instance>
