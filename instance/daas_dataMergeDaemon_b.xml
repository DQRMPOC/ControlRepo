<instance>
	<instance>daas_dataMergeDaemon_b</instance>
	<process>DS_FILEWATCHER</process>
	<description>File Watcher Process Template. Scans a number of directories based on Configuration. On Finding any file, a message will be sent to the consumers to notify them. If the file matches the regular expression in configuration, it will also try to process that file.</description>
	<inherit>0</inherit>
	<param>
		<parameter>dc_algroups</parameter>
		<paramvalue>daasPlatform,daasProcessing</paramvalue>
	</param>
	<param>
		<parameter>dc_gc</parameter>
		<paramvalue>Immediate</paramvalue>
	</param>
	<param>
		<parameter>dc_host</parameter>
		<paramvalue>ENV=daas_dataMergeDaemon_b_HOST=</paramvalue>
	</param>
	<param>
		<parameter>dc_instanceConfig</parameter>
		<paramvalue>DEFAULT</paramvalue>
	</param>
	<param>
		<parameter>dc_port</parameter>
		<paramvalue>ENV=daas_dataMergeDaemon_b_PORT=</paramvalue>
	</param>
	<param>
		<parameter>dc_schemas</parameter>
		<paramvalue>.daas.processing.pdInfo,dxFiles</paramvalue>
	</param>
	<param>
		<parameter>dc_taskset</parameter>
		<paramvalue>ENV=daas_dataMergeDaemon_b_TASKSET=</paramvalue>
	</param>
	<param>
		<parameter>fileListConfigName</parameter>
		<paramvalue>DS_FILEWATCHER_MATCHTABLE:dataMergeDaemon</paramvalue>
	</param>
	<param>
		<parameter>initialStateFunct</parameter>
		<paramvalue>.daas.process.init</paramvalue>
	</param>
	<param>
		<parameter>messagingServer</parameter>
		<paramvalue>DS_MESSAGING_SERVER:emea_tr_b</paramvalue>
	</param>
	<param>
		<parameter>pollInterval</parameter>
		<paramvalue>5</paramvalue>
	</param>
	<instancevariable>
		<variable>*</variable>
		<typ>object</typ>
	</instancevariable>
	<shutdown>
		<shutdownType>unexpected</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
	<shutdown>
		<shutdownType>proper</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
	<shutdown>
		<shutdownType>dc_timeoutAction</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
</instance>
