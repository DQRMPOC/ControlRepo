<instance>
	<instance>ds_tomcat_connect_start</instance>
	<process>DS_LAUNCH</process>
	<description>Launches the Tomcat Server</description>
	<inherit>0</inherit>
	<param>
		<parameter>blockOnCommand</parameter>
		<paramvalue>false</paramvalue>
	</param>
	<param>
		<parameter>command</parameter>
		<paramvalue>./startTomcatConnect.sh</paramvalue>
	</param>
	<param>
		<parameter>dc_gc</parameter>
		<paramvalue>Immediate</paramvalue>
	</param>
	<param>
		<parameter>dc_host</parameter>
		<paramvalue>ENV=ds_tomcat_connect_start_HOST=</paramvalue>
	</param>
	<param>
		<parameter>dc_port</parameter>
		<paramvalue>ENV=ds_tomcat_connect_start_PORT=</paramvalue>
	</param>
	<param>
		<parameter>dc_taskset</parameter>
		<paramvalue>ENV=ds_tomcat_connect_start_TASKSET=</paramvalue>
	</param>
	<param>
		<parameter>parameterConfig</parameter>
		<paramvalue>DS_LAUNCH_COMMANDLINE_PARAMS:ds_appServer</paramvalue>
	</param>
	<param>
		<parameter>runInBackground</parameter>
		<paramvalue>true</paramvalue>
	</param>
	<param>
		<parameter>workingDirectory</parameter>
		<paramvalue>ENV=DELTABIN_HOME=/bin</paramvalue>
	</param>
	<instancevariable>
		<variable>*</variable>
		<typ>object</typ>
	</instancevariable>
	<shutdown>
		<shutdownType>unexpected</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
	<shutdown>
		<shutdownType>proper</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
	<shutdown>
		<shutdownType>dc_timeoutAction</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
</instance>
