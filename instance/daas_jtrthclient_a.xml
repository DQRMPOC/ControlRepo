<instance>
	<instance>daas_jtrthclient_a</instance>
	<process>DS_LAUNCH</process>
	<description>External Process Launcher. Provides the ability to start external applications from Delta Control including applications that include the Delta Control API.</description>
	<inherit>0</inherit>
	<param>
		<parameter>blockOnCommand</parameter>
		<paramvalue>false</paramvalue>
	</param>
	<param>
		<parameter>command</parameter>
		<paramvalue>scripts/run.sh</paramvalue>
	</param>
	<param>
		<parameter>dc_gc</parameter>
		<paramvalue>Immediate</paramvalue>
	</param>
	<param>
		<parameter>dc_host</parameter>
		<paramvalue>ENV=daas_jtrthclient_a_HOST=</paramvalue>
	</param>
	<param>
		<parameter>dc_port</parameter>
		<paramvalue>ENV=daas_jtrthclient_a_PORT=</paramvalue>
	</param>
	<param>
		<parameter>dc_taskset</parameter>
		<paramvalue>ENV=daas_jtrthclient_a_TASKSET=</paramvalue>
	</param>
	<param>
		<parameter>parameterConfig</parameter>
		<paramvalue>DS_LAUNCH_COMMANDLINE_PARAMS:trthClient</paramvalue>
	</param>
	<param>
		<parameter>runInBackground</parameter>
		<paramvalue>true</paramvalue>
	</param>
	<param>
		<parameter>workingDirectory</parameter>
		<paramvalue>ENV=DAASTRTHCLIENT_HOME=/</paramvalue>
	</param>
	<instancevariable>
		<variable>*</variable>
		<typ>object</typ>
	</instancevariable>
	<shutdown>
		<shutdownType>unexpected</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
	<shutdown>
		<shutdownType>proper</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
	<shutdown>
		<shutdownType>dc_timeoutAction</shutdownType>
		<action></action>
		<publish>1</publish>
	</shutdown>
</instance>
